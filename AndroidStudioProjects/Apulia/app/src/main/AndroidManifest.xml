<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="it.apuliassicurazioni.mobile">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Tm11Activity"
            android:label="@string/label_tm11"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".C45Activity"
            android:label="@string/label_c45"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".NascitaActivity"
            android:label="@string/input_nascita" />
        <activity android:name=".StampaActivity" />
        <activity
            android:name=".M50Activity"
            android:label="@string/label_m50"
            android:parentActivityName=".MainActivity" />
    </application>

</manifest>